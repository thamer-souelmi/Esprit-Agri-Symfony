          <div class="col-lg-7">
                        <div class="p-5">
                            {{ form_start(form, {'action': path('app_traitementmedicale_edit', {'id': traitementmedicale.id}), 'attr': {'novalidate': 'novalidate'}}) }}

{{ form_row(form.numero, {'label' : 'Numéro de bétail','attr': {'class': 'form-control form-control-user', 'placeholder': 'Entrez le numéro du betail'}}) }}
<br>
{{ form_row(form.typeintervmed, {'label' : 'Traitement médical','attr': {'class': 'form-control form-control-user'}}) }}
<br>
{{ form_row(form.dateintervmed, {'label' : 'Date de traitement','attr': {'class': 'form-control form-control-user'}}) }}
<br>
{{ form_row(form.coutinterv, {'label' : 'Cout','attr': {'class': 'form-control form-control-user', 'placeholder': 'Entrez le cout du traitement médicale'}}) }}
<br>
{{ form_row(form.medicament, {'label' : 'Médicaments','attr': {'class': 'form-control form-control-user', 'placeholder': 'Entrez les médicaments'}}) }}
<br>
{{ form_row(form.dureetraitement, {'label' : 'Durée de traitement','attr': {'class': 'form-control form-control-user', 'placeholder': 'Entrez la durée du traitement medicale'}}) }}
<br>
{{ form_row(form.description, {'label' : 'Déscription','attr': {'class': 'form-control form-control-user', 'placeholder': 'Entrez la description de son état'}}) }}
<br>
{{ form_row(form.idvet, {'label' : 'Véterinaire','attr': {'class': 'form-control form-control-user'}}) }}


   {{ include('traitementmedicale/_form.html.twig', {'button_label': 'Modifier'}) }}        
{{ form_end(form) }}
   
